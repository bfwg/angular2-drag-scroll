{
  "name": "angular-drag-scroll",
  "version": "2.0.0-alpha.0",
  "description": "Lightweight drag to scroll directive for Angular2",
  "contributors": [
    {
      "name": "Fan Jin",
      "email": "fanjin1989@gmail.com"
    }
  ],
  "main": "index.js",
  "scripts": {
    "prepare": "npm run ngc",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --single-run",
    "clean:build": "npm run rimraf -- ./src/**/*.js ./src/**/*.d.ts ./src/**/*.map src/**/*.metadata.json index.js index.d.ts index.js.map index.metadata.json",
    "lint": "ng lint",
    "clean": "npm run rimraf -- node_modules && npm cache clean",
    "rimraf": "rimraf",
    "ngc": "ngc -p src/tsconfig.json",
    "tsc": "tsc -p src",
    "build:all": "ng build --prod && ng build --prod --app 1",
    "build:aot": "ng build --prod --aot && ng build --prod --aot --app 1",
    "build:optimizer": "ng build --prod --aot --build-optimizer && ng build --prod --aot --build-optimizer --app 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bfwg/angular2-drag-scroll.git"
  },
  "keywords": [
    "Angular 2",
    "drag",
    "scroll"
  ],
  "author": "Fan Jin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bfwg/angular2-drag-scroll.git"
  },
  "homepage": "https://github.com/bfwg/angular2-drag-scroll#readme",
  "peerDependencies": {
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
    "@angular/core": "^4.4.5"
  },
  "devDependencies": {
    "@angular/animations": "^4.4.5",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/cli": "1.4.7",
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
    "@angular/compiler-cli": "^4.4.5",
    "@angular/core": "^4.4.5",
    "@angular/forms": "^4.4.5",
    "@angular/http": "^4.4.5",
    "@angular/language-service": "^4.4.5",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.4.5",
    "@angular/platform-browser-dynamic": "^4.4.5",
    "@angular/platform-server": "^4.4.5",
    "@angular/router": "^4.4.5",
    "@angular/flex-layout": "2.0.0-beta.9",
    "@nguniversal/express-engine": "^1.0.0-beta.3",
    "@types/core-js": "^0.9.43",
    "@types/express": "^4.0.37",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^6.0.89",
    "@types/hammerjs": "^2.0.34",
    "hammerjs": "^2.0.8",
    "codelyzer": "~3.2.0",
    "compression": "^1.7.1",
    "core-js": "^2.5.1",
    "express": "^4.16.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "rimraf": "^2.6.2",
    "rxjs": "^5.4.3",
    "ts-node": "^3.3.0",
    "tslint": "~5.7.0",
    "typescript": "~2.3.3",
    "zone.js": "^0.8.18"
  }
}
