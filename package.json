  "main": "index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "prepare": "npm run ngc && copyfiles -f src/*.js src/*.js.map src/*.d.ts src/*.metadata.json .",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --single-run",
    "lint": "ng lint",
    "clean:build": "npm run rimraf -- **/*.js **/*.metadata.json **/*.d.ts **/*.map index.js index.d.ts index.js.map index.metadata.json",
    "clean": "npm run rimraf -- node_modules && npm cache clean",
    "rimraf": "rimraf",
    "ngc": "ngc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bfwg/angular2-drag-scroll.git"
